[Desktop Entry]
Type=Application
Name=Spotify (Adblock)
GenericName=Music Player
Icon=spotify-client
TryExec=spotify
Exec=env LD_PRELOAD="/usr/lib/spotify-xstub.so /usr/local/lib/spotify-adblock.so" spotify --enable-features=UseOzonePlatform --ozone-platform=wayland --uri=%U
Terminal=false
MimeType=x-scheme-handler/spotify;
Categories=Audio;Music;Player;AudioVideo;
StartupWMClass=spotify